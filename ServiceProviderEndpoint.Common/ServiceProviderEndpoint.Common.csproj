<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net6.0;net7.0;net8.0;net9.0</TargetFrameworks>
	<LangVersion>latest</LangVersion>
	<Nullable>enable</Nullable>
	<RootNamespace>ServiceProviderEndpoint</RootNamespace>
	<SignAssembly>True</SignAssembly>
	<AssemblyOriginatorKeyFile>..\ServiceProviderEndpoint.snk</AssemblyOriginatorKeyFile>
	<GeneratePackageOnBuild>false</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
  	<InternalsVisibleTo Include="ServiceProviderEndpoint" PublicKey="" />
  	<InternalsVisibleTo Include="ServiceProviderEndpoint.Client" PublicKey="" />
  </ItemGroup>
	
  <ItemGroup>
	<PackageReference Include="MetaFile" Version="1.1.1" />
  </ItemGroup>
	
  <ItemGroup Condition="'$(TargetFramework)' == 'netstandard2.0'">
  	<PackageReference Include="System.Text.Json" Version="9.0.0" />
  </ItemGroup>
	
</Project>
